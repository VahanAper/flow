{
  "extends": "airbnb",
  "plugins": [
      "react",
      "jsx-a11y",
      "import",
      "flow-vars"
  ],
  "env": {
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "linebreak-style": 0
  }
}
